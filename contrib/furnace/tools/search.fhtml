<% USING: help kernel sequences html namespaces io ; %>

<form action="search" method="get">
    <h1>Search help</h1>
    <input type="field" name="terms" />
    <%
        "terms" get dup empty? [
            drop
        ] [
            <b> "Results for " write dup write ":" write </b> <br/>
            search-help.
        ] if
    %>
    <input type="submit" />
</form>
